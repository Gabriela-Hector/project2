<div class="row justify-content-center">
    <div class="detalles-collab">

        <h4>Detalles de colaboración</h4>

        <div class="peticion-dos">
            <p><strong>Colaboración:</strong> {{collaborationType}}</p>
            <p><strong>Descripción:</strong> </p>
            <p><strong>Teléfono:</strong> {{telephoneNumber}}</p>
            <p><strong>Status</strong>: {{status}}</p>
        </div>
        <br>
        <section>
            <style>
                #collabMap {
                    width: 100%;
                    height: 200px;
                }
            </style>
            <figure collaborationId="{{_id}}" id="collabMap"></figure>
        </section>

        <div id="goback-button">
            <button onclick="history.back(-1)" class="myButton-dos">Volver</button>

            <script>
                function goBack() {
                    window.history.back();
                }
            </script>
        </div>

    </div>
</div>

<script src="/javascripts/collaboration/collaboration.map.js"></script>